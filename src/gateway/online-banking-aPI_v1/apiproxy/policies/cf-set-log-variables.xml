<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="cf-set-log-variables">
    <DisplayName>cf-set-log-variables</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>metadata.log.everything</Name>
        <Value>true</Value>
        <Ref/>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
